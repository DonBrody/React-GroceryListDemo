{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "react/prop-types": "off",
    "react/forbid-prop-types": "off",
    "react/destructuring-assignment": "off"
  }
}